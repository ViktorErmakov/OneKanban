0, 100, 0.5);\n    --tag-bg: lightgrey;\n    --tag-inactive-bg: rgb(233, 233, 233);\n    --tag-text: #333333;\n    --link-color: rgb(17, 138, 236);\n    --link-hover: rgb(17, 138, 236);\n    --command-bg: white;\n    --icon-color: #8c8c8c;\n    --icon-hover: #8c8c8c;\n    --button-bg: #f0f0f0;\n    --button-active-bg: #8c8c8c;\n    --button-text: rgb(17, 138, 236);\n    --dragover-bg: #e6f7ff;\n    --dragover-border: #91d5ff;\n    --header-hover: aliceblue;\n    --header-shadow: rgba(0, 0, 0, 0.1);\n    --add-task-bg: transparent;\n    --add-task-hover: #8c8c8c;\n}\n\n/* подключаем внешний шрифт */\n/*@import url("https://fonts.googleapis.com/css2?family=Inter&display=swap");*/\n\n/* общие настройки для всего */\nhtml {\n    /* font-size: 10px; */\n    font-size: 62.5%;\n    /* Это делает 1rem = 10px по умолчанию (16px * 0.625 = 10px) */\n    box-sizing: border-box;\n}\n\nbody {\n    margin: 0;\n    font-size: 1.7rem;\n    overflow: auto;\n    min-height: 100vh;\n    background-color: var(--bg-primary);\n    color: var(--text-primary);\n    transition: background-color 0.3s ease, color 0.3s ease;\n}\n\n/* Скрываем полосу прокрутки */\n.scrollable-container::-webkit-scrollbar {\n    width: 0;\n    /* Для вертикальной полосы */\n    /*height: 0;*/\n    /* Для горизонтальной полосы */\n}\n\n.wrapper {\n    color: var(--text-primary);\n    font-family: "Inter", sans-serif;\n    display: flex;\n    flex-direction: column;\n    background-color: var(--bg-primary);\n    min-height: 100vh;\n}\n\n.wrapper.dark-theme {\n    --bg-primary: #1a1a1a;\n    --bg-secondary: #242424;\n    --br-block: #1a1a1a;\n    --bg-tertiary: #333333;\n    --text-primary: #e0e0e0;\n    --text-secondary: #9a9a9a;\n    --text-tertiary: #a0a0a0;\n    --border-color: #3a3a3a;\n    --card-bg: #ffffff;\n    --card-border: #3a3a3a;\n    --card-shadow: rgba(0, 0, 0, 0.3);\n    --card-hover: #494949;\n    --card-hover-shadow: 0 0 12px rgba(90, 10, 180, 0.4), 0 0 20px rgba(70, 80, 160, 0.3);\n    --tag-bg: #4a4a4a;\n    --tag-inactive-bg: #383838;\n    --tag-text: #b0b0b0;\n    --link-color: #6fa8d8;\n    --link-hover: #8ab8e8;\n    --command-bg: #242424;\n    --icon-color: #8a8a8a;\n    --icon-hover: #b0b0b0;\n    --button-bg: #2a2a2a;\n    --button-active-bg: #5a6a7a;\n    --button-text: #6fa8d8;\n    --dragover-bg: #2f3d4d;\n    --dragover-border: #4a6a8a;\n    --header-hover: #333333;\n    --header-shadow: rgba(0, 0, 0, 0.3);\n    --add-task-bg: #3a3a3a;\n    --add-task-hover: #5a6a7a;\n}\n\n/* общие настройки для всего */\nhtml {\n    /* font-size: 10px; */\n    font-size: 62.5%;\n    /* Это делает 1rem = 10px по умолчанию (16px * 0.625 = 10px) */\n    box-sizing: border-box;\n}\n\nbody {\n    margin: 0;\n    font-size: 1.7rem;\n    overflow: auto;\n    height: 100vh;\n    background-color: var(--bg-primary);\n    color: var(--text-primary);\n    transition: background-color 0.3s ease, color 0.3s ease;\n}\n\n/* Скрываем полосу прокрутки */\n.scrollable-container::-webkit-scrollbar {\n    width: 0;\n    /* Для вертикальной полосы */\n    /*height: 0;*/\n    /* Для горизонтальной полосы */\n}\n\n.sticky_blok {\n    display: flex;\n    position: fixed;\n    top: 0;\n    flex-direction: column;\n    z-index: 100;\n    width: 100%;\n    transition: background-color 0.3s ease;\n}\n\n#kanban-board {\n    display: flex;\n    width: 100%;\n    flex: 1;\n}\n\n#kanban_header {\n    display: flex;\n    width: 100%;\n    min-height: 5.8rem;\n}\n\n#main-content-wrapper {\n    /* Чтобы масштабирование происходило из верхнего левого угла, а не из центра */\n    transform-origin: top left;\n    /* Добавьте transition для плавности масштабирования */\n    /* transition: transform 0.2s ease-in-out;  */\n}\n\n.block_header {\n    display: flex;\n    align-items: center;\n    border-top-left-radius: 1rem;\n    border-top-right-radius: 1rem;\n    border-top: var(--br-block) 1px solid;\n    background-color: var(--bg-secondary);\n    transition: background-color 0.3s ease;\n}\n\n.block_header:first-child {\n    margin-left: .5rem;\n}\n\n.kanban_body {\n    display: flex;\n    width: 100%;\n    min-height: calc(100vh - 5.8rem - 4rem);\n    /* Account for header height */\n    margin-top: 5.8rem;\n}\n\n.kanban-block {\n    margin-top: 11.4rem;\n    background-color: var(--bg-secondary);\n    transition: background-color 0.3s ease;\n    display: flex;\n    flex-direction: column;\n    min-height: 85vh;\n}\n\n.block_header,\n.kanban-block {\n    /* Для равномерного распределения колонок: */\n    flex: 1 1 0;\n    /* Позволяет элементам расти и занимать доступное пространство */\n    min-width: 0;\n    /* Важно для flex: 1, чтобы избежать переполнения */\n\n    padding: 0.6rem;\n    margin-right: .5rem;\n    /* box-sizing: border-box; */\n}\n\n.kanban-block:first-child {\n    margin-left: .5rem;\n}\n\n.kanban-block:hover {\n    background-color: var(--bg-tertiary);\n}\n\n/* Пример стилей для подсветки */\n.block_header--hovered {\n    background-color: var(--header-hover);\n    /* Более светлый фон */\n    box-shadow: 0 2px 5px var(--header-shadow);\n    /* Небольшая тень */\n    transition: background-color 0.2s ease, box-shadow 0.2s ease;\n    /* Плавный переход */\n}\n\n/* чекбоксы */\n/* скрываем флажок у чекбокса */\ninput[type="checkbox"] {\n    display: none;\n}\n\n.checkboxes {\n    display: flex;\n    flex-direction: row;\n    flex-wrap: wrap;\n    max-width: 60rem;\n    overflow-x: auto;\n    padding: 0.4rem;\n    margin-right: 1rem;\n    align-items: center;\n}\n\n/* замена gap на margin для совместимости с 1С */\n.checkboxes .checkbox {\n    margin: 0.25rem;\n}\n\n.checkboxes::-webkit-scrollbar {\n    height: 6px;\n}\n\n.checkboxes::-webkit-scrollbar-track {\n    background-color: transparent;\n}\n\n.checkboxes::-webkit-scrollbar-thumb {\n    background-color: var(--border-color);\n    border-radius: 3px;\n    transition: background-color 0.3s ease;\n}\n\n.checkboxes::-webkit-scrollbar-thumb:hover {\n    background-color: #999;\n}\n\n.checkbox {\n    padding: 0;\n    display: flex;\n    align-items: center;\n}\n\n.checkbox label {\n    color: var(--text-primary);\n    display: flex;\n    align-items: center;\n}\n\n.wrapper.dark-theme .checkbox,\n.wrapper.dark-theme .checkbox label {\n    color: #ffffff;\n}\n\n/* Тёмная тема — плашки проектов как у кнопки "Мои задачи" */\n/* Тёмная тема — project_pill как у кнопок "Мои задачи" и "Без группировки" */\n.wrapper.dark-theme .project_pill,\n.wrapper.dark-theme .project_grid_item {\n    background-color: var(--button-bg);\n    border-color: var(--border-color);\n    color: var(--text-primary);\n}\n\n/* Тёмная тема — выбранный проект как активная кнопка */\n.wrapper.dark-theme .project_grid_item.selected {\n    background-color: var(--button-active-bg);\n    color: #e0e0e0;\n    border-color: var(--button-active-bg);\n    filter: none;\n}\n\n/* общие настройки для тегов проектов (pill-стиль) */\n.tag {\n    /* внешние отступы */\n    padding: 0.4rem 1rem;\n    padding-left: 2.2rem; /* место для цветной точки */\n    /* подгоняем размер блока под содержимое */\n    width: -webkit-fit-content;\n    width: -moz-fit-content;\n    width: fit-content;\n    /* pill-форма */\n    border-radius: 1.6rem;\n    /* граница */\n    border: 1px solid var(--border-color);\n    /* запрещаем выделять мышкой этот элемент */\n    -webkit-user-select: none;\n    -moz-user-select: none;\n    -ms-user-select: none;\n    user-select: none;\n    background-color: var(--button-bg);\n    color: var(--text-primary);\n    font-size: 1.3rem;\n    transition: background-color 0.3s ease, color 0.3s ease, border-color 0.3s ease;\n    position: relative;\n    display: flex;\n    align-items: center;\n    white-space: nowrap;\n}\n\n/* цветная точка проекта слева */\n.tag::before {\n    content: "";\n    position: absolute;\n    left: 0.8rem;\n    width: 0.8rem;\n    height: 0.8rem;\n    border-radius: 50%;\n    background-color: var(--project-color, var(--tag-bg));\n    flex-shrink: 0;\n}\n\n/* кнопка закрытия × справа */\n.tag::after {\n    content: "×";\n    margin-left: 0.4rem;\n    font-size: 1.4rem;\n    opacity: 0.5;\n    transition: opacity 0.2s ease;\n    font-weight: normal;\n}\n\n.tag:hover::after {\n    opacity: 1;\n}\n\n.tag:hover {\n    background-color: var(--bg-tertiary);\n    border-color: var(--border-color);\n}\n\n/* оставляем тег чуть бледнее когда не выбран */\n.tag__inactive {\n    opacity: 0.5;\n    background-color: var(--tag-inactive-bg);\n    color: var(--tag-text);\n}\n\n/* точка остаётся яркой даже при неактивном теге */\n.tag__inactive::before {\n    opacity: 1;\n}\n\n.tag_task {\n    width: 1rem;\n    height: 1rem;\n    border-radius: 50%;\n}\n\n/* ========== PROJECT PICKER (Tag-picker с dropdown) ========== */\n.project_picker {\n    position: relative;\n    display: flex;\n    align-items: center;\n    max-width: 50rem;\n}\n\n/* Выбранные проекты (pills в ряд) */\n.project_picker_selected {\n    display: flex;\n    flex-wrap: nowrap;\n    align-items: center;\n    overflow: hidden;\n}\n\n/* Pill выбранного проекта — заливка цветом проекта */\n.project_pill {\n    display: flex;\n    align-items: center;\n    padding: 0.4rem 0.8rem;\n    margin: 0.2rem;\n    background-color: var(--project-color, var(--button-bg));\n    border: 1px solid transparent;\n    border-radius: 1.6rem;\n    font-size: 1.5rem;\n    color: var(--text-primary);\n    white-space: nowrap;\n    cursor: pointer;\n    position: relative;\n    transition: opacity 0.2s ease, filter 0.2s ease;\n    -webkit-user-select: none;\n    user-select: none;\n}\n\n/* Кнопка × */\n.project_pill_close {\n    margin-left: 0.5rem;\n    font-size: 1.6rem;\n    opacity: 0.6;\n    transition: opacity 0.2s ease;\n    line-height: 1;\n}\n\n.project_pill:hover .project_pill_close {\n    opacity: 1;\n}\n\n.project_pill:hover {\n    filter: brightness(0.9);\n}\n\n/* Кнопка открытия dropdown */\n.project_picker_toggle {\n    display: flex;\n    align-items: center;\n    padding: 0.4rem 0.8rem;\n    margin-left: 0.3rem;\n    background-color: var(--button-bg);\n    border: 1px solid var(--border-color);\n    border-radius: 1.6rem;\n    cursor: pointer;\n    font-size: 1.5rem;\n    color: var(--text-primary);\n    transition: background-color 0.2s ease;\n    -webkit-user-select: none;\n    user-select: none;\n}\n\n.project_picker_toggle:hover {\n    background-color: var(--bg-tertiary);\n}\n\n.project_picker_toggle .dropdown_arrow {\n    stroke: var(--text-secondary);\n    transition: transform 0.2s ease;\n}\n\n.project_picker.open .project_picker_toggle .dropdown_arrow {\n    transform: rotate(180deg);\n}\n\n/* Счётчик "+N" */\n.project_picker_more {\n    margin-right: 0.4rem;\n    font-weight: 500;\n}\n\n.project_picker_more:empty {\n    display: none;\n}\n\n.project_picker_more:empty + .dropdown_arrow {\n    margin-left: 0;\n}\n\n/* Dropdown с сеткой проектов */\n.project_picker_dropdown {\n    position: absolute;\n    top: calc(100% + 0.4rem);\n    left: 0;\n    min-width: 30rem;\n    max-width: 50rem;\n    background-color: var(--command-bg);\n    border: 1px solid var(--border-color);\n    border-radius: 0.8rem;\n    box-shadow: 0 4px 12px var(--card-shadow);\n    opacity: 0;\n    visibility: hidden;\n    transform: translateY(-0.5rem);\n    transition: all 0.2s ease;\n    z-index: 200;\n    padding: 0.8rem;\n}\n\n.project_picker.open .project_picker_dropdown {\n    opacity: 1;\n    visibility: visible;\n    transform: translateY(0);\n}\n\n/* Сетка проектов (2-3 колонки) */\n.project_picker_grid {\n    display: flex;\n    flex-wrap: wrap;\n}\n\n/* Элемент в сетке — заливка цветом проекта */\n.project_grid_item {\n    display: flex;\n    align-items: center;\n    padding: 0.5rem 1rem;\n    margin: 0.3rem;\n    background-color: var(--project-color, var(--button-bg));\n    border: 1px solid transparent;\n    border-radius: 1.6rem;\n    font-size: 1.5rem;\n    color: var(--text-primary);\n    cursor: pointer;\n    position: relative;\n    transition: filter 0.2s ease;\n    -webkit-user-select: none;\n    user-select: none;\n}\n\n.project_grid_item:hover {\n    filter: brightness(0.92);\n}\n\n/* Выбранный проект в сетке — лёгкое затемнение */\n.project_grid_item.selected {\n    filter: brightness(0.85);\n}\n\n.project_grid_item.selected:hover {\n    filter: brightness(0.8);\n}\n\n/* чекбоксы */\n\n/* КАРТОЧКИ */\n/* настройки карточки */\n.card {\n    /* цвет фона */\n    /* background-color: var(--card-bg); */\n    /* граница */\n    border: 1px solid var(--card-border);\n    /* рисуем границу по периметру карточки */\n    /* межстрочное расстояние */\n    line-height: 1.5;\n    /* радиус скругления */\n    border-radius: .8rem;\n    /* рисуем тень */\n    /* box-shadow: var(--card-shadow) 0px 1px 4px; */\n    /* скрываем всё, что не помещается в блок */\n    /* overflow: hidden; */\n    /* включаем гибкую вёрстку */\n    display: flex;\n    /* организуем вёрстку по столбцам */\n    flex-direction: column;\n    /* выравниваем карточки по центру друг относительно друга */\n    justify-content: space-between;\n    align-items: center;\n    margin-top: 1rem;\n    padding: .5rem;\n    color: var(--text-primary);\n\n    /* Обеспечиваем плавный переход для opacity и height */\n    transition: opacity 0.5s ease-out, height 0.5s ease-out, background-color 0.3s ease, border-color 0.3s ease;\n    /* Время и тип анимации */\n    overflow: hidden;\n    /* Важно! Скрывает контент, когда высота уменьшается */\n\n}\n\n/* меняем цвет рамки карточки при наведении */\n.card:hover {\n    background-color: var(--card-hover);\n    box-shadow: var(--card-shadow) 0px 1px 4px, var(--card-hover-shadow);\n}\n\n/* если тег не отмечен */\n.card__inactive {\n    opacity: 0;\n    /* Делаем невидимым */\n    height: 0;\n    /* Устанавливаем высоту в 0, чтобы элемент не занимал место */\n    padding-top: 0;\n    padding-bottom: 0;\n    margin-top: 0;\n    margin-bottom: 0;\n    border-width: 0;\n}\n\n/* КАРТОЧКИ */\nlabel {\n    cursor: pointer;\n    color: var(--text-primary);\n    transition: color 0.3s ease;\n}\n\n/* убираем hover opacity для label проектов - hover теперь на .tag */\n.checkbox label:hover {\n    opacity: 1;\n}\n\n.kanban-block__name {\n    margin-left: 1rem;\n    color: var(--text-secondary);\n}\n\n.kanban-block__number {\n    margin-left: 1rem;\n    color: var(--text-tertiary);\n}\n\n.card__header {\n    display: flex;\n    width: 100%;\n    justify-content: space-between;\n    align-items: start;\n}\n\n.card__link {\n    color: var(--link-color);\n    text-decoration: none;\n    transition: color 0.3s ease;\n}\n\n.card__link:hover {\n    text-decoration: underline;\n    color: var(--link-hover);\n}\n\n.card__photo {\n    width: 3.5rem;\n    height: 4rem;\n    border: 1px solid var(--border-color);\n    background-color: var(--bg-secondary);\n    transition: background-color 0.3s ease;\n}\n\n.card__photo:hover {\n    background-color: var(--bg-tertiary);\n    transform: scale(1.4);\n    position: relative;\n    right: 1rem;\n    top: 1rem;\n}\n\n.card__text {\n    display: flex;\n    width: 100%;\n    color: var(--text-primary);\n}\n\n.command_panel {\n    display: flex;\n    flex-direction: row;\n    align-items: center;\n    padding: .5rem;\n    background-color: var(--command-bg);\n    justify-content: space-between;\n    height: 10.3rem;\n    /* border-bottom: 1px solid var(--border-color); */\n    transition: background-color 0.3s ease, border-color 0.3s ease;\n}\n\n.command_panel_buttons {\n    display: flex;\n    flex-direction: row;\n    align-items: center;\n    justify-content: flex-end;\n    margin-left: auto;\n}\n\n.command_panel_buttons>* {\n    margin-right: 1rem;\n}\n\n.command_panel_buttons>*:last-child {\n    margin-right: 0;\n}\n\n#update_svg,\n#setup_svg {\n    display: flex;\n    justify-content: center;\n    align-items: center;\n    width: 3rem;\n    height: 3rem;\n    transition: transform 0.4s ease;\n    cursor: pointer;\n}\n\n#update_svg svg,\n#setup_svg svg {\n    fill: var(--icon-color);\n    stroke: var(--icon-color);\n    transition: fill 0.3s ease, stroke 0.3s ease;\n}\n\n/*Вращаем кнопку обновления на 180 градусов*/\n#update_svg:hover,\n#setup_svg:hover {\n    transform: rotate(180deg);\n}\n\n#update_svg:hover svg,\n#setup_svg:hover svg {\n    fill: var(--icon-hover);\n    stroke: var(--icon-hover);\n}\n\n/*Убираем вращение и увеличиваем кнопку*/\n#update_svg:active,\n#setup_svg:active {\n    transform: rotate(-180deg) scale(1.2);\n}\n\n.my_tasks {\n    color: var(--text-primary);\n    text-decoration: none;\n    font-family: 'Arial', sans-serif;\n    padding: 0.4rem 1rem;\n    border-radius: 1.6rem;\n    transition: all 0.3s ease;\n    display: inline-block;\n    border: 1px solid var(--border-color);\n    cursor: pointer;\n    -webkit-user-select: none;\n    user-select: none;\n    background-color: var(--button-bg);\n    font-size: 1.5rem;\n    white-space: nowrap;\n}\n\n.my_tasks:hover {\n    background-color: var(--bg-tertiary);\n    color: var(--text-primary);\n    border-color: var(--border-color);\n}\n\n/* Светлая тема — активная кнопка с затемнением как у выбранных фильтров */\n.my_tasks_active {\n    filter: brightness(0.85);\n    border-color: var(--border-color);\n}\n\n/* Тёмная тема — активная кнопка с цветным фоном */\n.wrapper.dark-theme .my_tasks_active {\n    background-color: var(--button-active-bg);\n    color: #e0e0e0;\n    border-color: var(--button-active-bg);\n    filter: none;\n}\n\n/* Переключатель темы */\n.theme_toggle_container {\n    display: flex;\n    align-items: center;\n    margin-right: 2rem;\n}\n\n.theme_toggle {\n    position: relative;\n    display: flex;\n    width: 5.4rem;\n    height: 2.8rem;\n    background-color: var(--border-color);\n    border-radius: 2.8rem;\n    cursor: pointer;\n    transition: background-color 0.3s ease;\n    border: none;\n    padding: 0;\n    align-items: center;\n    justify-content: center;\n}\n\n.theme_toggle.active {\n    background-