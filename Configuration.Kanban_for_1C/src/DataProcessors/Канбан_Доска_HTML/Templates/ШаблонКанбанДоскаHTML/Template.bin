0, 100, 0.5);\n    --tag-bg: lightgrey;\n    --tag-inactive-bg: rgb(233, 233, 233);\n    --tag-text: #333333;\n    --link-color: rgb(17, 138, 236);\n    --link-hover: rgb(17, 138, 236);\n    --command-bg: white;\n    --icon-color: #8c8c8c;\n    --icon-hover: #8c8c8c;\n    --button-bg: #f0f0f0;\n    --button-active-bg: #8c8c8c;\n    --button-text: rgb(17, 138, 236);\n    --dragover-bg: #e6f7ff;\n    --dragover-border: #91d5ff;\n    --header-hover: aliceblue;\n    --header-shadow: rgba(0, 0, 0, 0.1);\n    --add-task-bg: transparent;\n    --add-task-hover: #8c8c8c;\n}\n\n/* подключаем внешний шрифт */\n/*@import url("https://fonts.googleapis.com/css2?family=Inter&display=swap");*/\n\n/* общие настройки для всего */\nhtml {\n    /* font-size: 10px; */\n    font-size: 62.5%;\n    /* Это делает 1rem = 10px по умолчанию (16px * 0.625 = 10px) */\n    box-sizing: border-box;\n}\n\nbody {\n    margin: 0;\n    font-size: 1.7rem;\n    overflow: auto;\n    min-height: 100vh;\n    background-color: var(--bg-primary);\n    color: var(--text-primary);\n    transition: background-color 0.3s ease, color 0.3s ease;\n}\n\n/* Скрываем полосу прокрутки */\n.scrollable-container::-webkit-scrollbar {\n    width: 0;\n    /* Для вертикальной полосы */\n    /*height: 0;*/\n    /* Для горизонтальной полосы */\n}\n\n.wrapper {\n    color: var(--text-primary);\n    font-family: "Inter", sans-serif;\n    display: flex;\n    flex-direction: column;\n    background-color: var(--bg-primary);\n    min-height: 100vh;\n}\n\n.wrapper.dark-theme {\n    --bg-primary: #1a1a1a;\n    --bg-secondary: #242424;\n    --br-block: #1a1a1a;\n    --bg-tertiary: #333333;\n    --text-primary: #e0e0e0;\n    --text-secondary: #9a9a9a;\n    --text-tertiary: #a0a0a0;\n    --border-color: #3a3a3a;\n    --card-bg: #ffffff;\n    --card-border: #3a3a3a;\n    --card-shadow: rgba(0, 0, 0, 0.3);\n    --card-hover: #494949;\n    --card-hover-shadow: 0 0 12px rgba(90, 10, 180, 0.4), 0 0 20px rgba(70, 80, 160, 0.3);\n    --tag-bg: #4a4a4a;\n    --tag-inactive-bg: #383838;\n    --tag-text: #b0b0b0;\n    --link-color: #6fa8d8;\n    --link-hover: #8ab8e8;\n    --command-bg: #242424;\n    --icon-color: #8a8a8a;\n    --icon-hover: #b0b0b0;\n    --button-bg: #2a2a2a;\n    --button-active-bg: #5a6a7a;\n    --button-text: #6fa8d8;\n    --dragover-bg: #2f3d4d;\n    --dragover-border: #4a6a8a;\n    --header-hover: #333333;\n    --header-shadow: rgba(0, 0, 0, 0.3);\n    --add-task-bg: #3a3a3a;\n    --add-task-hover: #5a6a7a;\n}\n\n/* общие настройки для всего */\nhtml {\n    /* font-size: 10px; */\n    font-size: 62.5%;\n    /* Это делает 1rem = 10px по умолчанию (16px * 0.625 = 10px) */\n    box-sizing: border-box;\n}\n\nbody {\n    margin: 0;\n    font-size: 1.7rem;\n    overflow: auto;\n    height: 100vh;\n    background-color: var(--bg-primary);\n    color: var(--text-primary);\n    transition: background-color 0.3s ease, color 0.3s ease;\n}\n\n/* Скрываем полосу прокрутки */\n.scrollable-container::-webkit-scrollbar {\n    width: 0;\n    /* Для вертикальной полосы */\n    /*height: 0;*/\n    /* Для горизонтальной полосы */\n}\n\n.sticky_blok {\n    display: flex;\n    position: fixed;\n    top: 0;\n    flex-direction: column;\n    z-index: 100;\n    width: 100%;\n    transition: background-color 0.3s ease;\n}\n\n#kanban-board {\n    display: flex;\n    flex-direction: column;\n    width: 100%;\n    flex: 1;\n}\n\n#kanban_header {\n    display: flex;\n    width: 100%;\n    min-height: 5.8rem;\n}\n\n#main-content-wrapper {\n    /* Чтобы масштабирование происходило из верхнего левого угла, а не из центра */\n    transform-origin: top left;\n    /* Добавьте transition для плавности масштабирования */\n    /* transition: transform 0.2s ease-in-out;  */\n}\n\n.block_header {\n    display: flex;\n    align-items: center;\n    border-top-left-radius: 1rem;\n    border-top-right-radius: 1rem;\n    border-top: var(--br-block) 1px solid;\n    background-color: var(--bg-secondary);\n    transition: background-color 0.3s ease;\n}\n\n.block_header:first-child {\n    margin-left: .5rem;\n}\n\n.kanban_body {\n    display: flex;\n    width: 100%;\n    min-height: calc(100vh - 5.8rem - 4rem);\n    /* Account for header height */\n    margin-top: 5.8rem;\n}\n\n.kanban-block {\n    margin-top: 11.4rem;\n    background-color: var(--bg-secondary);\n    transition: background-color 0.3s ease;\n    display: flex;\n    flex-direction: column;\n    min-height: 85vh;\n}\n\n.block_header,\n.kanban-block {\n    /* Для равномерного распределения колонок: */\n    flex: 1 1 0;\n    /* Позволяет элементам расти и занимать доступное пространство */\n    min-width: 0;\n    /* Важно для flex: 1, чтобы избежать переполнения */\n\n    padding: 0.6rem;\n    margin-right: .5rem;\n    /* box-sizing: border-box; */\n}\n\n.kanban-block:first-child {\n    margin-left: .5rem;\n}\n\n.kanban-block:hover {\n    background-color: var(--bg-tertiary);\n}\n\n/* Пример стилей для подсветки */\n.block_header--hovered {\n    background-color: var(--header-hover);\n    /* Более светлый фон */\n    box-shadow: 0 2px 5px var(--header-shadow);\n    /* Небольшая тень */\n    transition: background-color 0.2s ease, box-shadow 0.2s ease;\n    /* Плавный переход */\n}\n\n/* чекбоксы */\n/* скрываем флажок у чекбокса */\ninput[type="checkbox"] {\n    display: none;\n}\n\n.checkboxes {\n    display: flex;\n    flex-direction: row;\n    max-width: 250rem;\n    overflow-x: auto;\n    /* Показывает прокрутку только при необходимости */\n    border: 2px solid var(--border-color);\n    border-radius: .4rem;\n    background-color: var(--bg-secondary);\n    transition: border-color 0.3s ease, background-color 0.3s ease;\n    /* min-height: 10rem; */\n    margin-right: 2rem;\n}\n\n.checkboxes::-webkit-scrollbar {\n    height: 8px;\n}\n\n.checkboxes::-webkit-scrollbar-track {\n    background-color: var(--bg-secondary);\n    transition: background-color 0.3s ease;\n}\n\n.checkboxes::-webkit-scrollbar-thumb {\n    background-color: var(--border-color);\n    border-radius: 4px;\n    transition: background-color 0.3s ease;\n}\n\n.checkboxes::-webkit-scrollbar-thumb:hover {\n    background-color: #999;\n}\n\n.checkbox {\n    padding: .5rem;\n}\n\n.checkbox label {\n    color: var(--text-primary);\n}\n\n.wrapper.dark-theme .checkbox,\n.wrapper.dark-theme .checkbox label {\n    color: #ffffff;\n}\n\n/* общие настройки для тегов */\n.tag {\n    /* внешние отступы */\n    padding: .6rem 1.2rem;\n    /*min-height: 2.5rem;*/\n    /* подгоняем размер блока под содержимое */\n    width: -webkit-fit-content;\n    width: -moz-fit-content;\n    width: fit-content;\n    /* радиус скругления */\n    border-radius: .4rem;\n    /* запрещаем выделять мышкой этот элемент */\n    -webkit-user-select: none;\n    -moz-user-select: none;\n    -ms-user-select: none;\n    /* user-select: none; */\n    background-color: var(--tag-bg);\n    color: var(--text-primary);\n    transition: background-color 0.3s ease, color 0.3s ease;\n}\n\n/* добавляем хештег перед тегом */\n.tag::before {\n    /* добавляем текст, отступ и прозрачность */\n    content: "#";\n    margin-right: .2rem;\n    opacity: 0.3;\n}\n\n/* оставляем тег чуть бледнее при нажатии */\n.tag__inactive {\n    opacity: 0.4;\n    background-color: var(--tag-inactive-bg);\n    color: var(--tag-text);\n}\n\n.tag_task {\n    width: 1rem;\n    height: 1rem;\n    border-radius: 50%;\n}\n\n/* чекбоксы */\n\n/* КАРТОЧКИ */\n/* настройки карточки */\n.card {\n    /* цвет фона */\n    /* background-color: var(--card-bg); */\n    /* граница */\n    border: 1px solid var(--card-border);\n    /* рисуем границу по периметру карточки */\n    /* межстрочное расстояние */\n    line-height: 1.5;\n    /* радиус скругления */\n    border-radius: .8rem;\n    /* рисуем тень */\n    /* box-shadow: var(--card-shadow) 0px 1px 4px; */\n    /* скрываем всё, что не помещается в блок */\n    /* overflow: hidden; */\n    /* включаем гибкую вёрстку */\n    display: flex;\n    /* организуем вёрстку по столбцам */\n    flex-direction: column;\n    /* выравниваем карточки по центру друг относительно друга */\n    justify-content: space-between;\n    align-items: center;\n    margin-top: 1rem;\n    padding: .5rem;\n    color: var(--text-primary);\n\n    /* Обеспечиваем плавный переход для opacity и height */\n    transition: opacity 0.5s ease-out, height 0.5s ease-out, background-color 0.3s ease, border-color 0.3s ease;\n    /* Время и тип анимации */\n    overflow: hidden;\n    /* Важно! Скрывает контент, когда высота уменьшается */\n\n}\n\n/* меняем цвет рамки карточки при наведении */\n.card:hover {\n    background-color: var(--card-hover);\n    box-shadow: var(--card-shadow) 0px 1px 4px, var(--card-hover-shadow);\n}\n\n/* если тег не отмечен */\n.card__inactive {\n    opacity: 0;\n    /* Делаем невидимым */\n    height: 0;\n    /* Устанавливаем высоту в 0, чтобы элемент не занимал место */\n    padding-top: 0;\n    padding-bottom: 0;\n    margin-top: 0;\n    margin-bottom: 0;\n    border-width: 0;\n}\n\n/* КАРТОЧКИ */\nlabel {\n    cursor: pointer;\n    color: var(--text-primary);\n    transition: color 0.3s ease;\n}\n\n/* делаем тег чуть бледнее при наведении */\nlabel:hover {\n    opacity: 0.7;\n    color: var(--text-primary);\n}\n\n.kanban-block__name {\n    margin-left: 1rem;\n    color: var(--text-secondary);\n}\n\n.kanban-block__number {\n    margin-left: 1rem;\n    color: var(--text-tertiary);\n}\n\n.card__header {\n    display: flex;\n    width: 100%;\n    justify-content: space-between;\n    align-items: start;\n}\n\n.card__link {\n    color: var(--link-color);\n    text-decoration: none;\n    transition: color 0.3s ease;\n}\n\n.card__link:hover {\n    text-decoration: underline;\n    color: var(--link-hover);\n}\n\n.card__photo {\n    width: 3.5rem;\n    height: 4rem;\n    border: 1px solid var(--border-color);\n    background-color: var(--bg-secondary);\n    transition: background-color 0.3s ease;\n}\n\n.card__photo:hover {\n    background-color: var(--bg-tertiary);\n    transform: scale(1.4);\n    position: relative;\n    right: 1rem;\n    top: 1rem;\n}\n\n.card__text {\n    display: flex;\n    width: 100%;\n    color: var(--text-primary);\n}\n\n.command_panel {\n    display: flex;\n    flex-direction: row;\n    align-items: center;\n    padding: .5rem;\n    background-color: var(--command-bg);\n    justify-content: space-between;\n    height: 10.3rem;\n    /* border-bottom: 1px solid var(--border-color); */\n    transition: background-color 0.3s ease, border-color 0.3s ease;\n}\n\n.command_panel_buttons {\n    display: flex;\n    flex-direction: row;\n    align-items: center;\n    justify-content: flex-end;\n    margin-left: auto;\n}\n\n.command_panel_buttons>* {\n    margin-right: 1rem;\n}\n\n.command_panel_buttons>*:last-child {\n    margin-right: 0;\n}\n\n#update_svg,\n#setup_svg {\n    display: flex;\n    justify-content: center;\n    align-items: center;\n    width: 3rem;\n    height: 3rem;\n    transition: transform 0.4s ease;\n    cursor: pointer;\n}\n\n#update_svg svg,\n#setup_svg svg {\n    fill: var(--icon-color);\n    stroke: var(--icon-color);\n    transition: fill 0.3s ease, stroke 0.3s ease;\n}\n\n/*Вращаем кнопку обновления на 180 градусов*/\n#update_svg:hover,\n#setup_svg:hover {\n    transform: rotate(180deg);\n}\n\n#update_svg:hover svg,\n#setup_svg:hover svg {\n    fill: var(--icon-hover);\n    stroke: var(--icon-hover);\n}\n\n/*Убираем вращение и увеличиваем кнопку*/\n#update_svg:active,\n#setup_svg:active {\n    transform: rotate(-180deg) scale(1.2);\n}\n\n.my_tasks {\n    color: var(--button-text);\n    text-decoration: none;\n    font-family: 'Arial', sans-serif;\n    padding: 8px 12px;\n    border-radius: 4px;\n    transition: all 0.3s ease;\n    display: inline-block;\n    border: 2px solid var(--border-color);\n    cursor: pointer;\n    -webkit-user-select: none;\n    background-color: var(--button-bg);\n}\n\n.my_tasks:hover {\n    background-color: var(--bg-tertiary);\n    color: var(--button-text);\n    border-color: var(--border-color);\n}\n\n.my_tasks_active {\n    transform: scale(0.98);\n    background-color: var(--button-active-bg);\n    color: #e0e0e0;\n    border-color: var(--border-color);\n}\n\n/* Переключатель темы */\n.theme_toggle_container {\n    display: flex;\n    align-items: center;\n    margin-right: 2rem;\n}\n\n.theme_toggle {\n    position: relative;\n    display: flex;\n    width: 5.4rem;\n    height: 2.8rem;\n    background-color: var(--border-color);\n    border-radius: 2.8rem;\n    cursor: pointer;\n    transition: background-color 0.3s ease;\n    border: none;\n    padding: 0;\n    align-items: center;\n    justify-content: center;\n}\n\n.theme_toggle.active {\n    background-color: var(--link-color);\n    justify-content: flex-end;\n}\n\n.theme_toggle::before {\n    content: '';\n    position: absolute;\n    width: 2.4rem;\n    height: 2.4rem;\n    border-radius: 50%;\n    transition: transform 0.3s ease;\n    margin: 0.06rem;\n    left: 0.2rem;\n    background-color: white;\n    background-image: url(${g});\n    background-repeat: no-repeat;\n    background-position: center;\n    background-size: 60%;\n}\n\n.theme_toggle.active::before {\n    transform: translateX(2.6rem);\n    background-image: url(${m});\n}\n\n/* кнопка добавления задач +++ */\n.add_task {\n    min-width: 26px;\n    min-height: 26px;\n    border-radius: 50%;\n    display: flex;\n    align-items: center;\n    justify-content: center;\n    transition: all 0.3s ease;\n    border: none;\n    margin: 1rem;\n    cursor: pointer;\n}\n\n.add_task * {\n    pointer-events: none;\n}\n\n.add_task:hover {\n    background-color: #8c8c8c;\n    transform: rotate(90deg);\n}\n\n.add_task:hover svg path {\n    stroke: white;\n}\n\n.add_task:active {\n    transform: scale(0.9) rotate(90deg);\n}\n\n.add_task svg {\n    width: 16px;\n    height: 16px;\n}\n\n/* кнопка добавления задач --- */\n\n/* Стили для Drag & Drop подсветки */\n.kanban-block--dragover {\n   