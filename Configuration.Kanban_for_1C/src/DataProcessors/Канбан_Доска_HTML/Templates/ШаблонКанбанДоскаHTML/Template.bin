}

/* общие настройки для всего */
html {
    /* font-size: 10px; */
    font-size: 62.5%;
    /* Это делает 1rem = 10px по умолчанию (16px * 0.625 = 10px) */
    box-sizing: border-box;
}

body {
    margin: 0;
    font-size: 1.7rem;
    overflow: auto;
    height: 100vh;
    background-color: var(--bg-primary);
    color: var(--text-primary);
    transition: background-color 0.3s ease, color 0.3s ease;
}

/* Скрываем полосу прокрутки */
.scrollable-container::-webkit-scrollbar {
    width: 0;
    /* Для вертикальной полосы */
    /*height: 0;*/
    /* Для горизонтальной полосы */
}

.sticky_blok {
    display: flex;
    position: fixed;
    top: 0;
    flex-direction: column;
    z-index: 100;
    width: 100%;
    background-color: var(--command-bg);
    transition: background-color 0.3s ease;
}

#kanban-board {
    display: flex;
    width: 100%;
    flex: 1;
}

/* Плейсхолдер "Выберите проект" в project_picker */
.project_picker_placeholder {
    color: var(--text-secondary);
    font-style: italic;
    font-size: 1.4rem;
    padding: 0.4rem 0.8rem;
    white-space: nowrap;
}

/* Состояние когда нет выбранных проектов */
.project_picker.no-selection {
    border-color: var(--border-color);
}

#kanban_header {
    display: flex;
    width: 100%;
    min-height: 5.8rem;
    background-color: var(--bg-primary);
}

#main-content-wrapper {
    /* Чтобы масштабирование происходило из верхнего левого угла, а не из центра */
    transform-origin: top left;
    /* Добавьте transition для плавности масштабирования */
    /* transition: transform 0.2s ease-in-out;  */
}

.block_header {
    display: flex;
    align-items: center;
    border-top-left-radius: 1rem;
    border-top-right-radius: 1rem;
    border-top: var(--br-block) 1px solid;
    background-color: var(--bg-secondary);
    transition: background-color 0.3s ease;
}

.block_header:first-child {
    margin-left: .5rem;
}

.kanban_body {
    display: flex;
    width: 100%;
    min-height: calc(100vh - 11.8rem);
    /* Account for fixed header height: kanban_header (5.8rem) + command_panel (6rem) */
    margin-top: 1rem;
}

.kanban-block {
    margin-top: 1.5rem;
    background-color: var(--bg-secondary);
    transition: background-color 0.3s ease;
    display: flex;
    flex-direction: column;
    min-height: calc(100vh - 11.8rem);
}

.block_header,
.kanban-block {
    /* Для равномерного распределения колонок: */
    flex: 1 1 0;
    /* Позволяет элементам расти и занимать доступное пространство */
    min-width: 0;
    /* Важно для flex: 1, чтобы избежать переполнения */

    padding: 0.6rem;
    margin-right: .5rem;
    /* box-sizing: border-box; */
}

.kanban-block:first-child {
    margin-left: .5rem;
}

.kanban-block:hover {
    background-color: var(--bg-tertiary);
}

/* Пример стилей для подсветки */
.block_header--hovered {
    background-color: var(--header-hover);
    /* Более светлый фон */
    box-shadow: 0 2px 5px var(--header-shadow);
    /* Небольшая тень */
    transition: background-color 0.2s ease, box-shadow 0.2s ease;
    /* Плавный переход */
}

/* чекбоксы */
/* скрываем флажок у чекбокса */
input[type="checkbox"] {
    display: none;
}

.checkboxes {
    display: flex;
    flex-direction: row;
    flex-wrap: wrap;
    max-width: 60rem;
    overflow-x: auto;
    padding: 0.4rem;
    margin-right: 1rem;
    align-items: center;
}

/* замена gap на margin для совместимости с 1С */
.checkboxes .checkbox {
    margin: 0.25rem;
}

.checkboxes::-webkit-scrollbar {
    height: 6px;
}

.checkboxes::-webkit-scrollbar-track {
    background-color: transparent;
}

.checkboxes::-webkit-scrollbar-thumb {
    background-color: var(--border-color);
    border-radius: 3px;
    transition: background-color 0.3s ease;
}

.checkboxes::-webkit-scrollbar-thumb:hover {
    background-color: #999;
}

.checkbox {
    padding: 0;
    display: flex;
    align-items: center;
}

.checkbox label {
    color: var(--text-primary);
    display: flex;
    align-items: center;
}

.wrapper.dark-theme .checkbox,
.wrapper.dark-theme .checkbox label {
    color: #ffffff;
}

/* Тёмная тема — project_pill и project_grid_item с заданным цветом проекта */
.wrapper.dark-theme .project_pill[style*="--project-color"],
.wrapper.dark-theme .project_grid_item[style*="--project-color"] {
    background-color: var(--project-color);
    border-color: transparent;
    color: var(--text-primary);
}

.wrapper.dark-theme .project_pill[style*="--project-color"]:hover,
.wrapper.dark-theme .project_grid_item[style*="--project-color"]:hover {
    filter: brightness(0.85);
}

/* Тёмная тема — выбранный проект с цветом: затемнение */
.wrapper.dark-theme .project_grid_item.selected[style*="--project-color"] {
    filter: brightness(0.75);
}

.wrapper.dark-theme .project_grid_item.selected[style*="--project-color"]:hover {
    filter: brightness(0.85);
}

/* Тёмная тема — project_pill и project_grid_item БЕЗ цвета (стили как у grouping_toggle) */
.wrapper.dark-theme .project_pill:not([style*="--project-color"]),
.wrapper.dark-theme .project_grid_item:not([style*="--project-color"]) {
    background-color: var(--button-bg);
    border: 1px solid var(--border-color);
    color: var(--text-primary);
}

.wrapper.dark-theme .project_pill:not([style*="--project-color"]):hover,
.wrapper.dark-theme .project_grid_item:not([style*="--project-color"]):hover {
    background-color: var(--bg-tertiary);
    border-color: var(--border-color);
}

/* Тёмная тема — выбранный проект без цвета: активный фон */
.wrapper.dark-theme .project_grid_item.selected:not([style*="--project-color"]) {
    background-color: var(--button-active-bg);
    color: #e0e0e0;
    border-color: var(--button-active-bg);
}

.wrapper.dark-theme .project_grid_item.selected:not([style*="--project-color"]):hover {
    filter: brightness(1.1);
}

/* Светлая тема — выбранный проект без цвета: затемнение фона */
.project_grid_item.selected:not([style*="--project-color"]) {
    filter: brightness(0.85);
}

/* общие настройки для тегов проектов (pill-стиль) */
.tag {
    /* внешние отступы */
    padding: 0.4rem 1rem;
    padding-left: 2.2rem; /* место для цветной точки */
    /* подгоняем размер блока под содержимое */
    width: -webkit-fit-content;
    width: -moz-fit-content;
    width: fit-content;
    /* pill-форма */
    border-radius: 1.6rem;
    /* граница */
    border: 1px solid var(--border-color);
    /* запрещаем выделять мышкой этот элемент */
    -webkit-user-select: none;
    -moz-user-select: none;
    -ms-user-select: none;
    user-select: none;
    background-color: var(--button-bg);
    color: var(--text-primary);
    font-size: 1.3rem;
    transition: background-color 0.3s ease, color 0.3s ease, border-color 0.3s ease;
    position: relative;
    display: flex;
    align-items: center;
    white-space: nowrap;
}

/* цветная точка проекта слева */
.tag::before {
    content: "";
    position: absolute;
    left: 0.8rem;
    width: 0.8rem;
    height: 0.8rem;
    border-radius: 50%;
    background-color: var(--project-color, var(--tag-bg));
    flex-shrink: 0;
}

/* кнопка закрытия × справа */
.tag::after {
    content: "×";
    margin-left: 0.4rem;
    font-size: 1.4rem;
    opacity: 0.5;
    transition: opacity 0.2s ease;
    font-weight: normal;
}

.tag:hover::after {
    opacity: 1;
}

.tag:hover {
    background-color: var(--bg-tertiary);
    border-color: var(--border-color);
}

/* оставляем тег чуть бледнее когда не выбран */
.tag__inactive {
    opacity: 0.5;
    background-color: var(--tag-inactive-bg);
    color: var(--tag-text);
}

/* точка остаётся яркой даже при неактивном теге */
.tag__inactive::before {
    opacity: 1;
}

.tag_task {
    width: 1rem;
    height: 1rem;
    border-radius: 50%;
    background-color: var(--project-color, transparent);
}

/* ========== PROJECT PICKER (Tag-picker с dropdown) ========== */
.project_picker {
    position: relative;
    display: flex;
    align-items: center;
    max-width: 50rem;
    background-color: var(--bg-tertiary);
    padding: 0.3rem 0.5rem;
    border-radius: 1.6rem;
    margin-right: 1rem;
}

/* Выбранные проекты (pills в ряд) */
.project_picker_selected {
    display: flex;
    flex-wrap: nowrap;
    align-items: center;
    overflow: hidden;
}

/* Pill выбранного проекта */
/* Светлая тема: если есть цвет проекта — используем его, иначе стандартный фон */
.project_pill {
    display: flex;
    align-items: center;
    padding: 0.4rem 0.8rem;
    margin: 0.2rem;
    background-color: var(--project-color, var(--button-bg));
    border: 1px solid var(--border-color);
    border-radius: 1.6rem;
    font-size: 1.5rem;
    color: var(--text-primary);
    white-space: nowrap;
    cursor: pointer;
    position: relative;
    transition: opacity 0.2s ease, filter 0.2s ease, background-color 0.3s ease, border-color 0.3s ease;
    -webkit-user-select: none;
    user-select: none;
}

/* Светлая тема: проекты с цветом — прозрачная граница */
.project_pill[style*="--project-color"] {
    border-color: transparent;
}

/* Кнопка × */
.project_pill_close {
    margin-left: 0.5rem;
    font-size: 1.6rem;
    opacity: 0.6;
    transition: opacity 0.2s ease;
    line-height: 1;
}

.project_pill:hover .project_pill_close {
    opacity: 1;
}

/* Светлая тема: hover для проектов с цветом */
.project_pill[style*="--project-color"]:hover {
    filter: brightness(0.9);
}

/* Светлая тема: hover для проектов без цвета */
.project_pill:not([style*="--project-color"]):hover {
    background-color: var(--bg-tertiary);
}

/* Кнопка открытия dropdown */
.project_picker_toggle {
    display: flex;
    align-items: center;
    padding: 0.4rem 0.8rem;
    margin-left: 0.3rem;
    background-color: var(--button-bg);
    border: 1px solid var(--border-color);
    border-radius: 1.6rem;
    cursor: pointer;
    font-size: 1.5rem;
    color: var(--text-primary);
    transition: background-color 0.2s ease;
    -webkit-user-select: none;
    user-select: none;
}

.project_picker_toggle:hover {
    background-color: var(--bg-tertiary);
}

.project_picker_toggle .dropdown_arrow {
    stroke: var(--text-secondary);
    transition: transform 0.2s ease;
}

.project_picker.open .project_picker_toggle .dropdown_arrow {
    transform: rotate(180deg);
}

/* Счётчик "+N" */
.project_picker_more {
    margin-right: 0.4rem;
    font-weight: 500;
}

.project_picker_more:empty {
    display: none;
}

.project_picker_more:empty + .dropdown_arrow {
    margin-left: 0;
}

/* Dropdown с сеткой проектов */
.project_picker_dropdown {
    position: absolute;
    top: calc(100% + 0.4rem);
    left: 0;
    min-width: 30rem;
    max-width: 50rem;
    background-color: var(--command-bg);
    border: 1px solid var(--border-color);
    border-radius: 0.8rem;
    box-shadow: 0 4px 12px var(--card-shadow);
    opacity: 0;
    visibility: hidden;
    transform: translateY(-0.5rem);
    transition: all 0.2s ease;
    z-index: 200;
    padding: 0.8rem;
}

.project_picker.open .project_picker_dropdown {
    opacity: 1;
    visibility: visible;
    transform: translateY(0);
}

/* Сетка проектов (2-3 колонки) */
.project_picker_grid {
    display: flex;
    flex-wrap: wrap;
}

/* Элемент в сетке — заливка цветом проекта */
.project_grid_item {
    display: flex;
    align-items: center;
    padding: 0.5rem 2.5rem 0.5rem 1rem;
    margin: 0.3rem;
    background-color: var(--project-color, var(--button-bg));
    border: 1px solid transparent;
    border-radius: 1.6rem;
    font-size: 1.5rem;
    color: var(--text-primary);
    cursor: pointer;
    position: relative;
    transition: filter 0.2s ease;
    -webkit-user-select: none;
